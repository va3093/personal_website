{
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    },
    {
      "src": "lambdas/**/*.ts",
      "use": "@now/node",
      "config": {
        "includeFiles": ["data/blogs/*.md"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.+)",
      "dest": "/pages/api/$1.ts"
    }
  ]
}
